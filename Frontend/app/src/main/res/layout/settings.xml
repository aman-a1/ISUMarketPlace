<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="ExtraText"
    android:background="@drawable/custom_button">

    <TextView
        android:id="@+id/title"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:text="Settings"
        android:textAlignment="center"
        android:textStyle="bold"
        android:elegantTextHeight="true"
        android:textSize="35sp"
        android:gravity="start"
        tools:ignore="RtlCompat,UnusedAttribute"
        />


    <ImageView

        android:id="@+id/profile"
        android:layout_width="87dp"
        android:layout_height="83dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="265dp"
        android:layout_marginRight="265dp"
        android:layout_marginBottom="517dp"
        android:clickable="true"
        android:focusable="true"
        android:src="@drawable/ic_baseline_emoji_people_24"
        tools:ignore="KeyboardInaccessibleWidget" />


    <TextView
        android:id="@+id/profileText"
        android:layout_width="87dp"
        android:layout_height="58dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="263dp"
        android:layout_marginRight="263dp"
        android:layout_marginBottom="460dp"
        android:clickable="true"
        android:focusable="true"
        android:gravity="center"
        android:text="Profile"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText" />

    <ImageView

        android:id="@+id/security"
        android:layout_width="87dp"
        android:layout_height="83dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="56dp"
        android:layout_marginRight="56dp"
        android:layout_marginBottom="521dp"
        android:clickable="true"
        android:focusable="true"
        android:src="@drawable/ic_baseline_security_24"
        tools:ignore="KeyboardInaccessibleWidget" />

    <TextView
        android:id="@+id/securityText"
        android:layout_width="87dp"
        android:layout_height="58dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="54dp"
        android:layout_marginRight="54dp"
        android:layout_marginBottom="464dp"
        android:gravity="center"
        android:text="Security"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        android:clickable="true"
        android:focusable="true"
        tools:ignore="HardcodedText,KeyboardInaccessibleWidget"
         />

    <ImageView
        android:id="@+id/signout"
        android:layout_width="87dp"
        android:layout_height="83dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="159dp"
        android:layout_marginRight="159dp"
        android:layout_marginBottom="123dp"
        android:clickable="true"
        android:focusable="true"
        android:src="@drawable/sign_out"
        tools:ignore="KeyboardInaccessibleWidget" />


    <TextView
        android:id="@+id/signoutText"
        android:layout_width="87dp"
        android:layout_height="58dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="157dp"
        android:layout_marginRight="157dp"
        android:layout_marginBottom="62dp"
        android:clickable="true"
        android:focusable="true"
        android:gravity="center"
        android:text="Sign Out"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/display"
        android:layout_width="87dp"
        android:layout_height="83dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="266dp"
        android:layout_marginRight="266dp"
        android:layout_marginBottom="304dp"
        android:clickable="true"
        android:focusable="true"
        android:src="@drawable/ic_twotone_4k_24"
        tools:ignore="KeyboardInaccessibleWidget" />

    <TextView
        android:id="@+id/displayText"
        android:layout_width="87dp"
        android:layout_height="58dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="265dp"
        android:layout_marginRight="265dp"
        android:layout_marginBottom="243dp"
        android:gravity="center"
        android:text="Display"
        android:textColor="#000000"
        android:textSize="20sp"
        android:clickable="true"
        android:focusable="true"
        android:textStyle="bold"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/delete"
        android:layout_width="87dp"
        android:layout_height="83dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="49dp"
        android:layout_marginRight="49dp"
        android:layout_marginBottom="295dp"
        android:clickable="true"
        android:focusable="true"
        android:src="@drawable/delete_forever_24"
        tools:ignore="KeyboardInaccessibleWidget" />

    <TextView
        android:id="@+id/deleteText"
        android:layout_width="87dp"
        android:layout_height="58dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="48dp"
        android:layout_marginRight="48dp"
        android:layout_marginBottom="237dp"
        android:gravity="center"
        android:clickable="true"
        android:focusable="true"
        android:text="Delete Account"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText" />

</RelativeLayout>
