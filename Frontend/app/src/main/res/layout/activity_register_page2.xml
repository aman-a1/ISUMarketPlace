<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Register.RegisterPage"
    android:background="#FFF"
    >

    <EditText
        android:id="@+id/Email"
        android:layout_width="358dp"
        android:drawableLeft="@drawable/email_yellow"
        android:layout_height="61dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/edit2"
        android:ems="10"
        android:hint="Email Address"
        android:inputType="textEmailAddress"
        android:paddingLeft="25sp"
        android:paddingRight="25sp"
        android:textAlignment="center"
        android:textColorHint="#FBAD3E"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/Pass"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText,MissingConstraints,RtlCompat,RtlHardcoded" />

    <EditText
        android:id="@+id/Pass"
        android:layout_width="358dp"
        android:layout_height="61dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/edit2"
        android:drawableLeft="@drawable/lock_yellow"
        android:ems="10"
        android:hint="Password"
        android:inputType="textPassword"
        android:textAlignment="center"
        android:textColorHint="#FBAD3E"
        android:textStyle="bold"
        android:paddingLeft="25sp"
        android:paddingRight="25sp"
        app:layout_constraintBottom_toTopOf="@+id/CPass"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText,MissingConstraints,RtlCompat,RtlHardcoded" />

    <EditText
        android:id="@+id/Name"
        android:drawableLeft="@drawable/person_yellow"
        android:layout_width="358dp"
        android:layout_height="61dp"
        android:layout_marginBottom="69dp"
        android:background="@drawable/edit2"
        android:ems="10"
        android:hint="First Name"
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:textColorHint="#FBAD3E"
        android:textSize="20dp"
        android:textStyle="bold"
        android:paddingLeft="25sp"
        android:paddingRight="25sp"
        app:layout_constraintBottom_toTopOf="@+id/Email"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.547"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText,MissingConstraints,RtlCompat,RtlHardcoded,SpUsage" />

    <EditText
        android:id="@+id/LastName"
        android:layout_width="358dp"
        android:layout_height="61dp"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="9dp"
        android:background="@drawable/edit2"
        android:drawableLeft="@drawable/person_yellow"
        android:ems="10"
        android:hint="Last Name"
        android:inputType="textPersonName"
        android:paddingLeft="25sp"
        android:paddingRight="25sp"
        android:textAlignment="center"
        android:textColorHint="#FBAD3E"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/Email"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.547"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Name"
        tools:ignore="HardcodedText,MissingConstraints,RtlCompat,RtlHardcoded,SpUsage" />

    <EditText
        android:id="@+id/CPass"
        android:layout_width="358dp"
        android:layout_height="61dp"
        android:layout_marginBottom="40dp"
        android:background="@drawable/edit2"
        android:ems="10"
        android:hint="Confirm Password"
        android:inputType="textPassword"
        android:textAlignment="center"
        android:textColorHint="#FBAD3E"
        android:textStyle="bold"
        android:paddingLeft="25sp"
        android:paddingRight="25sp"
        android:drawableLeft="@drawable/lock_yellow"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText,RtlCompat,RtlHardcoded" />

    <Button
        android:id="@+id/button"
        android:layout_width="291dp"
        android:layout_height="55dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/login_background2"
        android:text="Submit"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText,MissingConstraints" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/isu"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="ContentDescription"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:text="Register"
        android:textAlignment="center"
        android:textColor="#FBAD3E"
        android:paddingBottom="0dp"
        android:textSize="50dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/Name"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="HardcodedText,MissingConstraints,RtlCompat,SpUsage"
        tools:layout_editor_absoluteX="0dp" />

</androidx.constraintlayout.widget.ConstraintLayout>